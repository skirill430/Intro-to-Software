{
  "name": "shopping-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-client": "ng serve --configuration=development",
    "start-server": "cd server && go run main.go",
    "start": "concurrently --kill-others \"npm run start-client\" \"npm run start-server\"",
    "start_build": "npm run build:prod & npm run buildserver:prod",
    "build": "ng build --configuration=development",
    "test": "npm run test:headless & npm run testserver",
    "testserver": "cd server/testing && go test -v",
    "buildserver:prod": "cd server && env GOOS=linux GOARCH=amd64 go build -ldflags \"-X main.temp=1\"",
    "wipe-databases": "cd server && rm -r *.db && cd testing && rm -r *.db",
    "watch": "ng build --watch --configuration=development",
    "build:prod": "ng build --configuration=production",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "concurrently": "^7.6.0",
    "cypress": "^12.7.0",
    "ngx-cookie-service": "^15.0.0",
    "rxjs": "^7.8.0",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.0",
    "@angular/cli": "~15.1.0",
    "@angular/compiler-cli": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
